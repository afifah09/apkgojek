<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list>
            <item android:bottom="3dp" android:left="2dp">
                <shape>
                    <gradient android:angle="0"
                        android:endColor="@color/white" android:startColor="@color/green_goclub" />
                    <stroke android:width="1dp" android:color="@color/bayangan" />/>
                    <padding android:bottom="15dp" android:left="15dp"
                        android:right="15dp" android:top="15dp" />
                    <corners android:topLeftRadius="20dp"/>
                    <corners android:topRightRadius="20dp"/>
                </shape>
            </item>
        </layer-list>
    </item>

</selector>